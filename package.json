{
  "name": "@dpid/command-state-machine",
  "version": "0.1.0",
  "description": "Command-driven state machine for TypeScript",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
