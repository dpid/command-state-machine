  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": ["state-machine", "commands", "fsm", "game-development"],
  "license": "MIT"
}
